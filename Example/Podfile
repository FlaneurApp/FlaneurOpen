use_frameworks!

platform :ios, '9.0'

target 'FlaneurOpen_Example' do
  pod 'FlaneurOpen', :path => '../'

  # Google Places API
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'GoogleMaps'

  pod 'FlaneurImagePicker', :git => "https://github.com/FlaneurApp/FlaneurImagePicker.git", :branch => "swift4-support"

  target 'FlaneurOpen_Tests' do
    inherit! :search_paths

    pod 'Quick', '~> 1.2.0'
    pod 'Nimble', '~> 7.0.0'
    pod 'FBSnapshotTestCase'
    pod 'Nimble-Snapshots'
  end
end
